<!DOCTYPE html>
<html>
    <head>
        <title>
            Home | Server App
        </title>
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8;" />
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        <meta name="Referrer-Policy" value="no-referrer | same-origin" />
        @inject Microsoft.AspNetCore.Antiforgery.IAntiforgery Xsrf
        @functions {
            public string GetAntiXsrfToken() {
                return Xsrf.GetAndStoreTokens(Context).RequestToken;
            }
        }
        <meta name="xsrf" content="@GetAntiXsrfToken()" />
        <link rel="preconnect" href="https://fonts.gstatic.com">
        <link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap" rel="stylesheet">
    </head>
    <body>
        <noscript>
            Javascript is required to run this app
        </noscript>
        <main id="app-root"></main>

        <script type="application/javascript" src="/lib/js/app.bundle.js"></script>
        <script type="application/javascript" src="/lib/js/vendors~app.bundle.js"></script>
        <script type="application/javascript" src="/lib/js/vendors~app~portal.bundle.js"></script>
    </body>
</html>